<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_view_pais" model="ir.actions.act_window">
        <field name="name">Mantenimiento Modelo Pais</field>
        <field name="res_model">m.pais</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="action_view_registro" model="ir.actions.act_window">
        <field name="name">Registro Salidas</field>
        <field name="res_model">m.registro</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="menu_main"
        name="Menu Principal"
        sequence="10"/>

    <menuitem
        id="menu_pais"
        name="Mantenimiento Pais"
        action="my_module.action_view_pais"
        parent="menu_main"
    />

    <menuitem
        id="menu_registro"
        name="Registro Salidas"
        action="my_module.action_view_registro"
        parent="menu_main"
    />
</odoo>
