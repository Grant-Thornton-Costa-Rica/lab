<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="account_move_inherited_model" model="ir.ui.view">
        <field name="name">account.move.inherited</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <field name="invoice_date" position="after">
                <field name="metodo_pago" position="after" required="True"/>
            </field>
            <xpath expr = "// field [@ name = 'invoice_line_ids'] / tree / field [@ name = 'product_id']" position = "before" > 
                    <field name="codigo_cabys" position="after" required="True"/>
            </xpath> 
        </field>
    </record>

