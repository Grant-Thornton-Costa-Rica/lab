<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_company_inherited_model" model="ir.ui.view">
        <field name="name">res.company.inherited</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="before">
                <!-- <field name="ced" /> -->
            </field>
            <!-- <field name="ced" required="True"/> -->
        </field>
    </record>

    <!-- Action -->
    <record id="action_view_cedula" model="ir.actions.act_window">
        <field name="name">Modelo Tipo Cedula</field>
        <field name="res_model">m.cedula</field>
        <field name="view_mode">tree,form</field>
    </record>

    <!-- Menu -->
    <menuitem
        id="menu_cedula"
        name="Tipo Cedula"
        action="my_module.action_view_cedula"
        parent="menu_registro"
    />
</odoo>
